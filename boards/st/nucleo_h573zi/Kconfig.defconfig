config TFM_INITIAL_ATTESTATION_KEY
	default y

config TFM_DUMMY_PROVISIONING
	default n

endif # BUILD_WITH_TFM
	
config TFM_BOARD
	string
	default "stm/nucleo_h573zi"

endif # BOARD_NUCLEO_H573ZI
